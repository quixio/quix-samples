{
  "libraryItemId": "marimo",
  "name": "Marimo",
  "language": "docker",
  "tags": {
    "Category": ["Data Exploration"],
    "Type": ["Auxiliary Services"]
  },
  "shortDescription": "Run Marimo reactive notebooks in your pipeline.",
  "DefaultFile": "dockerfile",
  "EntryPoint": "dockerfile",
  "IconFile": "icon.png",
  "DeploySettings": {
    "DeploymentType": "Service",
    "CpuMillicores": 500,
    "MemoryInMb": 2000,
    "Replicas": 1,
    "PublicAccess": true,
    "UrlPrefix": "marimo",
    "State": {
      "Enabled": true,
      "Size": 1
    },
    "Network": {
      "ServiceName": "marimo",
      "Ports":
      [
        {
          "Port": 80,
          "TargetPort": 8080
        }
      ]
    }
  },
  "Variables": [
    {
      "Name": "MARIMO_MODE",
      "Type": "EnvironmentVariable",
      "InputType": "FreeText",
      "Description": "Mode to run Marimo in: 'run' (read-only notebook), 'edit' (editable notebook), 'browser' (file browser)",
      "DefaultValue": "edit",
      "Required": false
    },
    {
      "Name": "MARIMO_NOTEBOOK",
      "Type": "EnvironmentVariable",
      "InputType": "FreeText",
      "Description": "The notebook file to open (e.g., main.py)",
      "DefaultValue": "main.py",
      "Required": false
    }
  ]
}
